

@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Cafeteria Gerald</title>
    <link rel="icon" type="image/x-icon" href="~/Content/Img/Cafe.ico" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v5.12.1/js/all.js" crossorigin="anonymous"></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700" rel="stylesheet" type="text/css" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/Content/startbootstrap-agency-gh-pages/css/styles.css" rel="stylesheet" />
</head>
<body id="page-top">
    <!-- Navegacion-->
    <nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index","Home")">Cafeteria Gerald</a>
            <a class="navbar-brand js-scroll-trigger" href="#page-top"></a><button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">Menu<i class="fas fa-bars ml-1"></i></button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav text-uppercase ml-auto">
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Login", "Account")">Iniciar Sesion</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Menu", "Home")">Menu</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Orden", "Home")">Orden</a></li>


                </ul>
            </div>
        </div>
    </nav>
    <!-- Masthead-->
    <header class="masthead">
        <div class="container">
            <div class="masthead-heading ">Bienvenido a nuestra cafeteria!</div>

        </div>
    </header>
    <!-- Servicios-->
    <section class="page-section" id="services">
        <div class="container">
            <div class="text-center">
                <h2 class="section-heading text-uppercase">Contamos con diferentes alimentos:</h2>
                <br />
              

            </div>

            <div>
                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100" src="~/Content/img/frappe-de-magnum.jpg" alt="Frappe" style="height:500px; width:100%;">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="~/Content/img/Panini.jpg" alt="Panini" style="height:500px; width:100%;">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="~/Content/img/pay.jpg" alt="Pay" style="height:500px; width:100%;">
                        </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Anterior</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Siguiente</span>
                    </a>
                </div>
            </div>

            <br />
            <h2 class="lead text-center">Si deseas revisar nuestros platillos da clic en el boton de abajo</h2>
            <div class="row justify-content-center">
                <a href="@Url.Action("Menu", "Home")" class="btn btn-success">Ir al menu</a>
            </div>

        </div>
    </section>

    <!-- Acerca de-->
    <section class="page-section" id="about">
        <div class="container">
            <div class="text-center">
                <h2 class="section-heading text-uppercase">Acerca de</h2>
                <br />


            </div>
            <div class="text-center">
                <p class="lead">
                    Somos una Cafeteria 100% mexicana la cual nos gusta brindar cafe artesanal,
                    asi como diferentes paltillos exquisitos los cuales los puedes disfrutar con tu pareja, amigos y familia.
                    seguimos brindando servicio desde 2009.
                </p>
                <p class="lead font-weight-bold">
                    Seguimos brindando servicio desde 2009.

                </p>

            </div>

            <img src="~/Content/Img/cafeteria acerca de.jpg" class=" img-fluid rounded " alt="Evento" style="  width: 60%; height:75%;  display: block;       margin-left: auto;  margin-right: auto;">


        </div>

    </section>
    <br />
    <br />



    <!-- Contacto-->
    <section class="page-section" id="contact">
        <div class="container">
            <div class="text-center">
                <h2 class="section-heading text-uppercase">Contacto</h2>
                <br />

            </div>
            <form id="contactForm" name="sentMessage" novalidate="novalidate">
                <div class="row align-items-stretch mb-5">
                    <div class="col-md-6">
                        <div class="form-group">
                            <input class="form-control" id="name" type="text" placeholder="Tu nombre *" required="required" data-validation-required-message="Por favor ingresa tu nombre." />
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                            <input class="form-control" id="email" type="email" placeholder="Tu correo *" required="required" data-validation-required-message="Por favor ingresa tu correo." />
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group mb-md-0">
                            <input class="form-control" id="phone" type="tel" placeholder="Tu numero telefonico *" required="required" data-validation-required-message="Por favor ingresa tu numero." />
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group form-group-textarea mb-md-0">
                            <textarea class="form-control" id="message" placeholder="Tu mensaje *" required="required" data-validation-required-message="Por favor ingresa tu mensaje."></textarea>
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                </div>
                <div class="text-center">
                    <div id="success"></div>
                    <button class="btn btn-success btn-xl text-uppercase" id="sendMessageButton" type="submit">Envia tu mensaje</button>
                </div>
            </form>
        </div>
    </section>
    <!-- Footer-->
    <footer class="footer py-4">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-4 text-lg-left">Copyright © Cafeteria Gerald 2020</div>
                <div class="col-lg-4 my-3 my-lg-0">
                    <a class="btn btn-info btn-social mx-2" href="#!"><i class="fab fa-twitter"></i></a><a class="btn btn-info btn-social mx-2" href="#!"><i class="fab fa-facebook-f"></i></a><a class="btn btn-info btn-social mx-2" href="#!"><i class="fab fa-linkedin-in"></i></a>
                </div>

            </div>
        </div>
    </footer>

    <!-- Bootstrap core JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Contact form JS-->
    <script src="~/Content/startbootstrap-agency-gh-pages/assets/mail/jqBootstrapValidation.js"></script>
    <script src="~/Content/startbootstrap-agency-gh-pages/assets/mail/contact_me.js"></script>
    <!-- Core theme JS-->
    <script src="~/Content/startbootstrap-agency-gh-pages/js/scripts.js"></script>
</body>
</html>
